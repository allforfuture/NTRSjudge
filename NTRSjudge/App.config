<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
  </startup>
  <appSettings>
    <!--方格设置：行个数、列个数-->
    <add key="row" value="3" />
    <add key="col" value="1" />
    <!--S/N检查的字符-->
    <add key="checkPPP" value="GH9,123" />
    <add key="checkEEEE" value="N/A" />
    <!--显示线别异常开关-->
    <add key="lineSwitch" value="0"/>
    <!--是否生成新格式（KK13）CSV文件-->
    <add key="newFormat" value="1"/>
    <!--csv字符-->
    <add key="type" value="KK08" />
    <add key="factory" value="NSTD" />
    <add key="building" value="M-1" />
    <add key="line" value="L09" />
    <add key="process" value="HNTRS" />
    <add key="inspect" value="HNTRS" />
    <!--API的工序和地址-->
    <add key="assy_cd" value="BONDING" />
    <add key="work_mode" value="INLINE" />
    <add key="API" value="http://172.27.42.14/vc/ok2aoi_ee149/json.php" />
  </appSettings>
</configuration>